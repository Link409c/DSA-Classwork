public Class BinaryTree<E>{
     
     




     //root is a node
     //nodes have left and right pointers
     private Node<E> root;
     
     public BinaryTree(){}

     public BinaryTree(E data, Node<E> leftTree, 
               Node<E> rightTree){

          Node<E> root = new Node<>;
          root.setE(data);
          root.setPrev(leftTree);
          root.setNext(rightTree);
          this.root = root;

     }
     
     public Node<E> getLeftSubtree(){
          return getRoot.getPrev();
     }
     
     public Node<E> getRightSubtree(){
          return getRoot.getNext();
     }

     public E getData(){
          return getRoot.getE();
     }

}
